package com.project.bookstack.entities;

import jakarta.persistence.*;
import java.io.Serializable;

@Entity
@Table(name = "book_genre")
public class BookGenre {

    @Id
    @Column(name = "book_id")
    private Integer bookId;

    @Id
    @Column(name = "genre_id")
    private Integer genreId;
    
}
